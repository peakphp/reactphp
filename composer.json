{
    "name": "peak/reactphp",
    "description": "Peak Application Using ReactPHP",
    "keywords": ["peak", "framework", "app", "reactphp", "event-driven"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "peak/framework": "@dev",
        "react/event-loop": "^1.1",
        "react/http": "^0.8.4",
        "zendframework/zend-diactoros": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "files": [
            "src/constants.php",
            "src/functions.php"
        ],
        "psr-4": {
            "App\\": "src/App/"
        }
    }
}
